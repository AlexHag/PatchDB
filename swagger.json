        ],
        "type": "object",
        "properties": {
          "fileId": {
            "type": "string",
            "format": "uuid"
          },
          "url": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "GetUserPatchesResponse": {
        "type": "object",
        "properties": {
          "patches": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserPatchModel"
            },
            "nullable": true
          },
          "unmatchesPatches": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserPatchUploadModel"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "NewMatchingPatchesModel": {
        "required": [
          "imageUrl"
        ],
        "type": "object",
        "properties": {
          "patchNumber": {
            "type": "integer",
            "format": "int32"
          },
          "imageUrl": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "patchMaker": {
            "type": "string",
            "nullable": true
          },
          "university": {
            "$ref": "#/components/schemas/UniversityModel"
          },
          "universitySection": {
            "type": "string",
            "nullable": true
          },
          "releaseDate": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "patchSubmissionId": {
            "type": "string",
            "format": "uuid"
          },
          "created": {
            "type": "string",
            "format": "date-time"
          },
          "updated": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "similarity": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "OwnedMatchingPatchesModel": {
        "required": [
          "matchingPatch",
          "uploads"
        ],
        "type": "object",
        "properties": {
          "userPatchId": {
            "type": "string",
            "format": "uuid"
          },
          "matchingPatch": {
            "$ref": "#/components/schemas/PatchResponse"
          },
          "uploads": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserPatchUploadModel"
            },
            "nullable": true
          },
          "isFavorite": {
            "type": "boolean"
          },
          "aquiredAt": {
            "type": "string",
            "format": "date-time"
          },
