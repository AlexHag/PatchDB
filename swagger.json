        "type": "object",
        "properties": {
          "user": {
            "$ref": "#/components/schemas/UserResponse"
          },
          "credentials": {
            "$ref": "#/components/schemas/AccessTokenModel"
          }
        },
        "additionalProperties": false
      },
      "AuthenticationMethod": {
        "enum": [
          "Password"
        ],
        "type": "string"
      },
      "FileUploadUrlResponse": {
        "required": [
